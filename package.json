{
  "name": "react-poe-canvas-chat-template",
  "version": "0.1.0",
  "type": "module",
  "author": "Rizean <rizean@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "ph-src": "ph --code src --ignore \"**/*.test.ts\"",
    "ph-src-extra": "ph --code src --code index.html --code vite.config.ts --ignore \"**/*.test.ts\"",
    "ph-src-with-test": "ph --code src",
    "ph-dep": "ph --dependency-graph",
    "ph-complexity": "ph --complexity"
  },
  "dependencies": {
    "@rizean/poe-canvas-utils": "^0.3.1",
    "marked": "^15.0.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "styled-components": "^6.1.18",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prompt-helper": "^2.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-singlefile": "^2.2.0"
  }
}
